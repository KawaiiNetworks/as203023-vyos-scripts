delete policy as-path-list AS-Tier1
set policy as-path-list AS-Tier1 rule 10 action 'permit'
set policy as-path-list AS-Tier1 rule 10 description 'Tier 1 ASNs'
set policy as-path-list AS-Tier1 rule 10 regex '^([0-9]+_)*(7018|3320|3257|6830|3356|2914|5511|3491|6453|6762|1299|12956|701|6461)(_[0-9]+)*$'
set policy as-path-list AS-Tier1 rule 20 action 'permit'
set policy as-path-list AS-Tier1 rule 20 description 'Tier 1 like ASNs'
set policy as-path-list AS-Tier1 rule 20 regex '^([0-9]+_)*(7473|174|6939|9002|1273|2828|4637|7922)(_[0-9]+)*$'
