delete policy prefix-list LOCAL-ALLOWED-PREFIX4
set policy prefix-list LOCAL-ALLOWED-PREFIX4 rule 10 action 'permit'
set policy prefix-list LOCAL-ALLOWED-PREFIX4 rule 10 description 'ipv4 bogons used in my network'
set policy prefix-list LOCAL-ALLOWED-PREFIX4 rule 10 le '32'
set policy prefix-list LOCAL-ALLOWED-PREFIX4 rule 10 prefix '10.203.0.0/16'
set policy prefix-list LOCAL-ALLOWED-PREFIX4 rule 20 action 'permit'
set policy prefix-list LOCAL-ALLOWED-PREFIX4 rule 20 description 'ipv4 bogons with AP'
set policy prefix-list LOCAL-ALLOWED-PREFIX4 rule 20 ge '24'
set policy prefix-list LOCAL-ALLOWED-PREFIX4 rule 20 le '24'
set policy prefix-list LOCAL-ALLOWED-PREFIX4 rule 20 prefix '192.168.0.0/16'
