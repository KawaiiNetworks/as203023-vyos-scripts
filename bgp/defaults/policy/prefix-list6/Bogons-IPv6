delete policy prefix-list6 Bogons-IPv6
set policy prefix-list6 Bogons-IPv6 rule 10 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 10 description 'Unspecified address'
set policy prefix-list6 Bogons-IPv6 rule 10 prefix '::/128'
set policy prefix-list6 Bogons-IPv6 rule 20 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 20 description 'Loopback address'
set policy prefix-list6 Bogons-IPv6 rule 20 prefix '::1/128'
set policy prefix-list6 Bogons-IPv6 rule 30 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 30 description 'IPv4-mapped addresses'
set policy prefix-list6 Bogons-IPv6 rule 30 le '128'
set policy prefix-list6 Bogons-IPv6 rule 30 prefix '::ffff:0:0/96'
set policy prefix-list6 Bogons-IPv6 rule 40 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 40 description 'IPv4 translated addresses'
set policy prefix-list6 Bogons-IPv6 rule 40 le '128'
set policy prefix-list6 Bogons-IPv6 rule 40 prefix '::ffff:0:0:0/96'
set policy prefix-list6 Bogons-IPv6 rule 50 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 50 description 'IPv4/IPv6 translation'
set policy prefix-list6 Bogons-IPv6 rule 50 le '128'
set policy prefix-list6 Bogons-IPv6 rule 50 prefix '64:ff9b::/96'
set policy prefix-list6 Bogons-IPv6 rule 60 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 60 description 'IPv4/IPv6 translation'
set policy prefix-list6 Bogons-IPv6 rule 60 le '128'
set policy prefix-list6 Bogons-IPv6 rule 60 prefix '64:ff9b:1::/48'
set policy prefix-list6 Bogons-IPv6 rule 70 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 70 description 'Discard prefix'
set policy prefix-list6 Bogons-IPv6 rule 70 le '128'
set policy prefix-list6 Bogons-IPv6 rule 70 prefix '100::/64'
set policy prefix-list6 Bogons-IPv6 rule 80 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 80 description 'Teredo tunneling'
set policy prefix-list6 Bogons-IPv6 rule 80 le '128'
set policy prefix-list6 Bogons-IPv6 rule 80 prefix '2001:0000::/32'
set policy prefix-list6 Bogons-IPv6 rule 90 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 90 description 'ORCHIDv2'
set policy prefix-list6 Bogons-IPv6 rule 90 le '128'
set policy prefix-list6 Bogons-IPv6 rule 90 prefix '2001:20::/28'
set policy prefix-list6 Bogons-IPv6 rule 100 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 100 description 'Addresses used in documentation and example source code'
set policy prefix-list6 Bogons-IPv6 rule 100 le '128'
set policy prefix-list6 Bogons-IPv6 rule 100 prefix '2001:db8::/32'
set policy prefix-list6 Bogons-IPv6 rule 110 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 110 description 'The 6to4 addressing scheme (deprecated)'
set policy prefix-list6 Bogons-IPv6 rule 110 le '128'
set policy prefix-list6 Bogons-IPv6 rule 110 prefix '2002::/16'
set policy prefix-list6 Bogons-IPv6 rule 120 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 120 description 'RFC 4193 unique local unicast'
set policy prefix-list6 Bogons-IPv6 rule 120 le '128'
set policy prefix-list6 Bogons-IPv6 rule 120 prefix 'fc00::/7'
set policy prefix-list6 Bogons-IPv6 rule 130 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 130 description 'RFC 4291 link local unicast'
set policy prefix-list6 Bogons-IPv6 rule 130 le '128'
set policy prefix-list6 Bogons-IPv6 rule 130 prefix 'fe80::/10'
set policy prefix-list6 Bogons-IPv6 rule 140 action 'permit'
set policy prefix-list6 Bogons-IPv6 rule 140 description 'RFC 4291 multicast'
set policy prefix-list6 Bogons-IPv6 rule 140 le '128'
