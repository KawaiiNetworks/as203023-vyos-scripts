delete policy prefix-list6 LOCAL-ALLOWED-PREFIX6
set policy prefix-list6 LOCAL-ALLOWED-PREFIX6 rule 10 action 'permit'
set policy prefix-list6 LOCAL-ALLOWED-PREFIX6 rule 10 description 'ipv6 bogons used in my network'
set policy prefix-list6 LOCAL-ALLOWED-PREFIX6 rule 10 le '128'
set policy prefix-list6 LOCAL-ALLOWED-PREFIX6 rule 10 prefix 'fd00:20:3023::/48'
set policy prefix-list6 LOCAL-ALLOWED-PREFIX6 rule 20 action 'permit'
set policy prefix-list6 LOCAL-ALLOWED-PREFIX6 rule 20 description 'ipv6 bogons with AP'
set policy prefix-list6 LOCAL-ALLOWED-PREFIX6 rule 20 ge '64'
set policy prefix-list6 LOCAL-ALLOWED-PREFIX6 rule 20 le '64'
set policy prefix-list6 LOCAL-ALLOWED-PREFIX6 rule 20 prefix 'fd00:192:168::/48'
