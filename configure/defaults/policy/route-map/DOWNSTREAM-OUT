delete policy route-map DOWNSTREAM-OUT
set policy route-map DOWNSTREAM-OUT rule 1 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 1 match protocol 'static'
set policy route-map DOWNSTREAM-OUT rule 1 on-match goto '110'
set policy route-map DOWNSTREAM-OUT rule 2 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 2 match protocol 'connected'
set policy route-map DOWNSTREAM-OUT rule 2 on-match goto '110'
set policy route-map DOWNSTREAM-OUT rule 10 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 10 match large-community large-community-list 'FROM-LOCAL-AS'
set policy route-map DOWNSTREAM-OUT rule 10 on-match goto '110'
set policy route-map DOWNSTREAM-OUT rule 26 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 26 match large-community large-community-list 'FROM-DOWNSTREAM'
set policy route-map DOWNSTREAM-OUT rule 26 on-match goto '150'
set policy route-map DOWNSTREAM-OUT rule 27 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 27 match large-community large-community-list 'FROM-PEER'
set policy route-map DOWNSTREAM-OUT rule 27 on-match goto '150'
set policy route-map DOWNSTREAM-OUT rule 28 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 28 match large-community large-community-list 'FROM-ROUTESERVER'
set policy route-map DOWNSTREAM-OUT rule 28 on-match goto '150'
set policy route-map DOWNSTREAM-OUT rule 29 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 29 match large-community large-community-list 'FROM-UPSTREAM'
set policy route-map DOWNSTREAM-OUT rule 29 on-match goto '150'
set policy route-map DOWNSTREAM-OUT rule 30 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 30 match large-community large-community-list 'FROM-IBGP'
set policy route-map DOWNSTREAM-OUT rule 30 on-match goto '150'
set policy route-map DOWNSTREAM-OUT rule 100 action 'deny'
set policy route-map DOWNSTREAM-OUT rule 110 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 110 call 'FILTER-ALLOW-LOCAL-ASN-PREFIX'
set policy route-map DOWNSTREAM-OUT rule 110 on-match goto '200'
set policy route-map DOWNSTREAM-OUT rule 150 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 150 call 'FILTER-EBGP'
set policy route-map DOWNSTREAM-OUT rule 150 on-match next
set policy route-map DOWNSTREAM-OUT rule 200 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 200 set large-community delete 'FROM-IBGP'
set policy route-map DOWNSTREAM-OUT rule 200 on-match next
set policy route-map DOWNSTREAM-OUT rule 201 action 'permit'
set policy route-map DOWNSTREAM-OUT rule 201 set large-community delete 'FROM-LOCAL-AS'
